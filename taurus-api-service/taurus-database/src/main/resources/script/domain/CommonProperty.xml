    <property global="true" name="Color" value="VARCHAR(7)" dbms="mssql"/>
    <property global="true" name="Color" value="VARCHAR(7)" dbms="mysql"/>
    <property global="true" name="Color" value="VARCHAR(7)" dbms="oracle"/>
    <property global="true" name="Color" value="VARCHAR(7)" dbms="sqlite"/>

    <!--身份证-->
    <property global="true" name="IdCard" value="VARCHAR(18)" dbms="mssql"/>
    <property global="true" name="IdCard" value="VARCHAR(18)" dbms="mysql"/>
    <property global="true" name="IdCard" value="VARCHAR2(18)" dbms="oracle"/>
    <property global="true" name="IdCard" value="VARCHAR(18)" dbms="sqlite"/>

    <!-- Address住址 -->
    <property global="true" name="Address" value="NVARCHAR(128)" dbms="mssql"/>
    <property global="true" name="Address" value="VARCHAR(128)" dbms="mysql"/>
    <property global="true" name="Address" value="NVARCHAR2(128)" dbms="oracle"/>
    <property global="true" name="Address" value="VARCHAR(128)" dbms="sqlite"/>

    <!--URL资源路径-->
    <property global="true" name="URL" value="NVARCHAR(2048)" dbms="mssql"/>
    <property global="true" name="URL" value="VARCHAR(2048)" dbms="mysql"/>
    <property global="true" name="URL" value="NVARCHAR2(2048)" dbms="oracle"/>
    <property global="true" name="URL" value="VARCHAR(2048)" dbms="sqlite"/>

