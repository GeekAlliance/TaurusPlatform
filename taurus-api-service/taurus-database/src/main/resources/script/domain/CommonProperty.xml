<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
				   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">

    <!--*****************全局属性：通用数据类型、数据库兼容配置**********************-->
    <!--*****************************通用数据类型*******************************-->
    <!--主键编号-->
    <property global="true" name="ID" value="VARCHAR(32)" dbms="mssql"/>
    <property global="true" name="ID" value="VARCHAR(32)" dbms="mysql"/>
    <property global="true" name="ID" value="VARCHAR2(32)" dbms="oracle"/>
    <property global="true" name="ID" value="VARCHAR(32)" dbms="sqlite"/>

    <!--字典编码-->
    <property global="true" name="Code" value="VARCHAR(32)" dbms="mssql"/>
    <property global="true" name="Code" value="VARCHAR(32)" dbms="mysql"/>
    <property global="true" name="Code" value="VARCHAR2(32)" dbms="oracle"/>
    <property global="true" name="Code" value="VARCHAR(32)" dbms="sqlite"/>

    <!--布尔-->
    <property global="true" name="Boole" value="CHAR(1)" dbms="mssql"/>
    <property global="true" name="Boole" value="CHAR(1)" dbms="mysql"/>
    <property global="true" name="Boole" value="CHAR(1)" dbms="oracle"/>
    <property global="true" name="Boole" value="CHAR(1)" dbms="sqlite"/>

    <!--小整数-->
    <property global="true" name="SmallInt" value="SMALLINT" dbms="mssql"/>
    <property global="true" name="SmallInt" value="SMALLINT" dbms="mysql"/>
    <property global="true" name="SmallInt" value="NUMBER(5,0)" dbms="oracle"/>
    <property global="true" name="SmallInt" value="SMALLINT" dbms="sqlite"/>

    <!--整数-->
    <property global="true" name="Int" value="INT" dbms="mssql"/>
    <property global="true" name="Int" value="INT" dbms="mysql"/>
    <property global="true" name="Int" value="INT" dbms="oracle"/>
    <property global="true" name="Int" value="INT" dbms="sqlite"/>

    <!--大整数-->
    <property global="true" name="BigInt" value="BIGINT" dbms="mssql"/>
    <property global="true" name="BigInt" value="BIGINT" dbms="mysql"/>
    <property global="true" name="BigInt" value="NUMBER" dbms="oracle"/>
    <property global="true" name="BigInt" value="BIGINT" dbms="sqlite"/>

    <!--近似值-->
    <property global="true" name="Float" value="FLOAT" dbms="mssql"/>
    <property global="true" name="Float" value="FLOAT" dbms="mysql"/>
    <property global="true" name="Float" value="FLOAT" dbms="oracle"/>
    <property global="true" name="Float" value="FLOAT" dbms="sqlite"/>

    <!--单字符-->
    <property global="true" name="Char" value="VARCHAR(1)" dbms="mssql"/>
    <property global="true" name="Char" value="VARCHAR(1)" dbms="mysql"/>
    <property global="true" name="Char" value="VARCHAR(1)" dbms="oracle"/>
    <property global="true" name="Char" value="VARCHAR(1)" dbms="sqlite"/>

    <!--短字符-->
    <property global="true" name="ShortChar" value="VARCHAR(128)" dbms="mssql"/>
    <property global="true" name="ShortChar" value="VARCHAR(128)" dbms="mysql"/>
    <property global="true" name="ShortChar" value="VARCHAR2(128)" dbms="oracle"/>
    <property global="true" name="ShortChar" value="VARCHAR(128)" dbms="sqlite"/>

    <!--中字符-->
    <property global="true" name="MiddleChar" value="VARCHAR(256)" dbms="mssql"/>
    <property global="true" name="MiddleChar" value="VARCHAR(256)" dbms="mysql"/>
    <property global="true" name="MiddleChar" value="VARCHAR2(256)" dbms="oracle"/>
    <property global="true" name="MiddleChar" value="VARCHAR(256)" dbms="sqlite"/>

    <!--长字符-->
    <property global="true" name="LongChar" value="VARCHAR(512)" dbms="mssql"/>
    <property global="true" name="LongChar" value="VARCHAR(512)" dbms="mysql"/>
    <property global="true" name="LongChar" value="VARCHAR2(512)" dbms="oracle"/>
    <property global="true" name="LongChar" value="VARCHAR(512)" dbms="sqlite"/>

    <!--超长字符-->
    <property global="true" name="SuperChar" value="TEXT" dbms="mssql"/>
    <property global="true" name="SuperChar" value="TEXT" dbms="mysql"/>
    <property global="true" name="SuperChar" value="CLOB" dbms="oracle"/>
    <property global="true" name="SuperChar" value="TEXT" dbms="sqlite"/>

    <!--字符串短-->
    <property global="true" name="ShortString" value="NVARCHAR(512)" dbms="mssql"/>
    <property global="true" name="ShortString" value="VARCHAR(512)" dbms="mysql"/>
    <property global="true" name="ShortString" value="NVARCHAR2(512)" dbms="oracle"/>
    <property global="true" name="ShortString" value="VARCHAR(512)" dbms="sqlite"/>

