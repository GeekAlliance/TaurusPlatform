    <property global="true" name="MiddleString" value="NVARCHAR(1024)" dbms="mssql"/>
    <property global="true" name="MiddleString" value="VARCHAR(1024)" dbms="mysql"/>
    <property global="true" name="MiddleString" value="NVARCHAR2(1024)" dbms="oracle"/>
    <property global="true" name="MiddleString" value="VARCHAR(1024)" dbms="sqlite"/>

