    <property global="true" name="AuthCode" value="VARCHAR(6)" dbms="mssql"/>
    <property global="true" name="AuthCode" value="VARCHAR(6)" dbms="mysql"/>
    <property global="true" name="AuthCode" value="VARCHAR2(6)" dbms="oracle"/>
    <property global="true" name="AuthCode" value="VARCHAR(6)" dbms="sqlite"/>

    <!--备注/描述信息-->
    <property global="true" name="Remark" value="NVARCHAR(512)" dbms="mssql"/>
    <property global="true" name="Remark" value="VARCHAR(512)" dbms="mysql"/>
    <property global="true" name="Remark" value="NVARCHAR2(512)" dbms="oracle"/>
    <property global="true" name="Remark" value="VARCHAR(512)" dbms="sqlite"/>

