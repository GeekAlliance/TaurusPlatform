    <property global="true" name="LongString" value="NVARCHAR(3072)" dbms="mssql"/>
    <property global="true" name="LongString" value="VARCHAR(3072)" dbms="mysql"/>
    <property global="true" name="LongString" value="NVARCHAR2(3072)" dbms="oracle"/>
    <property global="true" name="LongString" value="VARCHAR(3072)" dbms="sqlite"/>

    <!--文本-->
    <property global="true" name="Text" value="NTEXT" dbms="mssql"/>
    <property global="true" name="Text" value="TEXT" dbms="mysql"/>
    <property global="true" name="Text" value="NCLOB" dbms="oracle"/>
    <property global="true" name="Text" value="TEXT" dbms="sqlite"/>

    <!--TOKEN-->
    <property global="true" name="Token" value="TEXT" dbms="mssql"/>
    <property global="true" name="Token" value="BLOB" dbms="mysql"/>
    <property global="true" name="Token" value="NCLOB" dbms="oracle"/>
    <property global="true" name="Token" value="TEXT" dbms="sqlite"/>

    <!--数值-->
    <property global="true" name="Numerical" value="DECIMAL(15,6)" dbms="mssql"/>
    <property global="true" name="Numerical" value="DECIMAL(15,6)" dbms="mysql"/>
    <property global="true" name="Numerical" value="NUMBER(15,6)" dbms="oracle"/>
    <property global="true" name="Numerical" value="NUMBER(15,6)" dbms="sqlite"/>

    <!--通用值-->
    <property global="true" name="CommonValue" value="NVARCHAR(256)" dbms="mssql"/>
    <property global="true" name="CommonValue" value="VARCHAR(256)" dbms="mysql"/>
    <property global="true" name="CommonValue" value="NVARCHAR2(256)" dbms="oracle"/>
    <property global="true" name="CommonValue" value="VARCHAR(256)" dbms="sqlite"/>

    <!--类型（Int）-->
    <property global="true" name="TypeInt" value="SMALLINT" dbms="mssql"/>
    <property global="true" name="TypeInt" value="SMALLINT" dbms="mysql"/>
    <property global="true" name="TypeInt" value="NUMBER(5,0)" dbms="oracle"/>
    <property global="true" name="TypeInt" value="SMALLINT" dbms="sqlite"/>

    <!--类型（编码）-->
    <property global="true" name="TypeCode" value="VARCHAR(32)" dbms="mssql"/>
    <property global="true" name="TypeCode" value="VARCHAR(32)" dbms="mysql"/>
    <property global="true" name="TypeCode" value="VARCHAR2(32)" dbms="oracle"/>
    <property global="true" name="TypeCode" value="VARCHAR(32)" dbms="sqlite"/>

    <!--名称-->
    <property global="true" name="Name" value="NVARCHAR(128)" dbms="mssql"/>
    <property global="true" name="Name" value="VARCHAR(128)" dbms="mysql"/>
    <property global="true" name="Name" value="NVARCHAR2(128)" dbms="oracle"/>
    <property global="true" name="Name" value="VARCHAR(128)" dbms="sqlite"/>

    <!--IP地址-->
    <property global="true" name="IP" value="VARCHAR(23)" dbms="mssql"/>
    <property global="true" name="IP" value="VARCHAR(23)" dbms="mysql"/>
    <property global="true" name="IP" value="VARCHAR2(23)" dbms="oracle"/>
    <property global="true" name="IP" value="VARCHAR(23)" dbms="sqlite"/>

    <!--密码-->
    <property global="true" name="Password" value="VARCHAR(64)" dbms="mssql"/>
    <property global="true" name="Password" value="VARCHAR(64)" dbms="mysql"/>
    <property global="true" name="Password" value="VARCHAR2(64)" dbms="oracle"/>
    <property global="true" name="Password" value="VARCHAR(64)" dbms="sqlite"/>

    <!--密钥-->
    <property global="true" name="Secret" value="VARCHAR(256)" dbms="mssql"/>
    <property global="true" name="Secret" value="VARCHAR(256)" dbms="mysql"/>
    <property global="true" name="Secret" value="VARCHAR2(256)" dbms="oracle"/>
    <property global="true" name="Secret" value="VARCHAR(256)" dbms="sqlite"/>

    <!--工号-->
    <property global="true" name="WorkNo" value="VARCHAR(32)" dbms="mssql"/>
    <property global="true" name="WorkNo" value="VARCHAR(32)" dbms="mysql"/>
    <property global="true" name="WorkNo" value="VARCHAR2(32)" dbms="oracle"/>
    <property global="true" name="WorkNo" value="VARCHAR(32)" dbms="sqlite"/>

    <!--序列码-->
    <property global="true" name="SerialNo" value="VARCHAR(32)" dbms="mssql"/>
    <property global="true" name="SerialNo" value="VARCHAR(32)" dbms="mysql"/>
    <property global="true" name="SerialNo" value="VARCHAR2(32)" dbms="oracle"/>
    <property global="true" name="SerialNo" value="VARCHAR(32)" dbms="sqlite"/>

    <!--微信-->
    <property global="true" name="WeChat" value="VARCHAR(20)" dbms="mssql"/>
    <property global="true" name="WeChat" value="VARCHAR(20)" dbms="mysql"/>
    <property global="true" name="WeChat" value="VARCHAR2(20)" dbms="oracle"/>
    <property global="true" name="WeChat" value="VARCHAR(20)" dbms="sqlite"/>

    <!--时间、日期、日期时间-->
    <property global="true" name="DateTime" value="DATETIME" dbms="mssql"/>
    <property global="true" name="DateTime" value="DATETIME(3)" dbms="mysql"/>
    <property global="true" name="DateTime" value="DATE" dbms="oracle"/>
    <property global="true" name="DateTime" value="DATETIME" dbms="sqlite"/>

    <!--比例-->
    <property global="true" name="Ratio" value="DECIMAL(4,2)" dbms="mssql"/>
    <property global="true" name="Ratio" value="DECIMAL(4,2)" dbms="mysql"/>
    <property global="true" name="Ratio" value="NUMBER(4,2)" dbms="oracle"/>
    <property global="true" name="Ratio" value="DECIMAL(4,2)" dbms="sqlite"/>

    <!--温度-->
    <property global="true" name="Temperature" value="FLOAT" dbms="mssql"/>
    <property global="true" name="Temperature" value="FLOAT" dbms="mysql"/>
    <property global="true" name="Temperature" value="FLOAT" dbms="oracle"/>
    <property global="true" name="Temperature" value="FLOAT" dbms="sqlite"/>

    <!--电话-->
    <property global="true" name="Phone" value="VARCHAR(11)" dbms="mssql"/>
    <property global="true" name="Phone" value="VARCHAR(11)" dbms="mysql"/>
    <property global="true" name="Phone" value="NVARCHAR2(11)" dbms="oracle"/>
    <property global="true" name="Phone" value="VARCHAR(11)" dbms="sqlite"/>

